{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "dockerapp",
        "image": "jzhao5/flaskapp",
        "hostname": "dockerapp",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 5000
          }
        ],
        "memory": 128,
        "environment" : [
            { "name" : "redis", "value" : "myappdemo.aw3gvq.0001.use1.cache.amazonaws.com" },
            { "name" : "port", "value": "6379" }
        ]
      }
    ]
  }
  