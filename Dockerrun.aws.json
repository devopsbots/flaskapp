{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "dockerapp",
        "image": "jzhao5/flaskapp",
        "hostname": "dockerapp",
        "essential": true,
        "portMappings": [
          {
            "hostPort": 80,
            "containerPort": 5000
          }
        ],
        "links": ["redis"],
        "memory": 128,
        "environment" : [
            { "name" : "REDIS_HOST", "value" : "myappdemo.aw3gvq.0001.use1.cache.amazonaws.com" },
            { "name" : "REDIS_PORT", "value": "6379" },
        ]
      },
      {
        "name": "redis",
        "image": "redis",
        "hostname": "redis",
        "essential": true,
        "memory": 128
      }
    ]
  }
  