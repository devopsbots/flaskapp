{
    "AWSEBDockerrunVersion": 2,
    "containerDefinitions": [
      {
        "name": "dockerapp",
        "image": "jzhao5/flaskapp",
        "hostname": "dockerapp",
        "essential": true,
        "portMappings": [
            {
              "hostPort": 80,
              "containerPort": 5000
            }
        ],
        "memory": 128
      },
      {
        "name": "redis",
        "image": "redis:3.2.0",
        "hostname": "redis",
        "essential": true,
        "memory": 128
      }
    ]
  }
  